# AWS credentials
access_key = "your_aws_access_key"
secret_key = "your_aws_secret_key"


# AWS region
region = "us-west-2"


# The path to a public key that will be used to create an AWS key pair (named
# terraform by default). This key will be used to provision the machines after
# they are created.
#
# NOTE: To ensure provisioning works correctly, the corresponding private key
# should be added to your SSH agent, e.g, `ssh-add ~/.ssh/id_rsa`
public_key_path = "~/.ssh/id_rsa.pub"


site_name = "aws-test"

# vim: set ft=terraform:
